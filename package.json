{
	"name": "dvelop",
	
	"description": "Helps you make vidja games.",
	"copyright": "Copyright Â© 2013",
	"authors": [ "Colden Cullen" ],
	"dependencies": {
		"derelict-gl3": "~master",
		"derelict-fi": "~master",
		"dyaml": "~master"
	},
	"importPaths": [
		"source/",
		"third_party/DWinProgramming/WindowsAPI"
	],
	"configurations": [
		{
			"name": "windows-app",
			"platforms": [ "windows" ],
			"targetName": "Dvelop",
			"targetType": "executable",
			"mainSourceFile": "source/app.d",
			"libs": [ 
				"gdi32", "user32",
				"ole32", "kernel32", "user32", "comctl32", "comdlg32" 
			]
		},
		{
			"name": "osx-app",
			"platforms": [ "osx" ],
			"targetName": "Dvelop",
			"targetType": "executable",
			"mainSourceFile": "source/app.d"
		}
	],
	"targetPath": "bin/"
}
